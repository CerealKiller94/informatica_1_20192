      <block id="10" type="CPU">
        <options displayComment="false"/>
        <visual color="255 255 0" posX="260.0" posY="320.0"/>
        <content>sol1 = (((-1)*b) + Math.sqrt(discriminante))/(2*a);	sol2 = (((-1)*b) - Math.sqrt(discriminante))/(2*a)</content>
