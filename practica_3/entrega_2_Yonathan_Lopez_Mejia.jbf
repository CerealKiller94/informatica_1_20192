      <block id="6" type="CPU">
        <options displayComment="false"/>
        <visual color="255 255 0" posX="0.0" posY="140.0"/>
        <content>discriminante = Math.pow(b, 2) - (4*a*c)</content>
        <connect ID="7"/>
