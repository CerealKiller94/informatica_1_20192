      <block id="5" type="COMMENT">
        <options displayComment="false"/>
        <visual color="255 255 0" posX="-640.0" posY="-90.0"/>
        <content>Nombre: Yonathan López Mejia;	Grupo: 10;	Documento: 1017220389;	Enunciado: Determinar el área y volumen de un cilindro, dada su altura y su radio.;	;	Análisis:;	Entradas: radio (numero ;	que representa el radio del cilindro);	altura: (número que representa la altura del cilindro);	;	salida: area (número que representa el área del cilindro);	volumen (número que representa el volumen del cilindro);	;	;	</content>
      </block>
      <block id="6" type="IO">
        <options displayComment="true"/>
        <visual color="255 255 0" posX="0.0" posY="-310.0"/>
        <content>radio=ReadNumber("Digite el radio del cilindro");	;	</content>
        <comment>"Digite el radio del cilindro";	radio ;	</comment>
        <connect ID="11"/>
        <io array="0" inputType="0" ioType="2" message="Digite el radio del cilindro" variable="radio"/>
      </block>
      <block id="7" type="IO">
        <options displayComment="true"/>
        <visual color="255 255 0" posX="-10.0" posY="-130.0"/>
        <content>altura=ReadNumber("Digite la altura del cilindro");	;	</content>
        <comment>"Digite la altura del cilindro";	altura ;	</comment>
        <connect ID="14"/>
        <io array="0" inputType="0" ioType="2" message="Digite la altura del cilindro" variable="altura"/>
      </block>
      <block id="8" type="CPU">
        <options displayComment="false"/>
        <visual color="255 255 0" posX="0.0" posY="40.0"/>
        <content>area = 2*Math.PI*radio*(radio + altura);	volumen = Math.PI*Math.pow(radio, 2)*altura</content>
        <connect ID="9"/>
      </block>
      <block id="9" type="IO">
        <options displayComment="true"/>
        <visual color="255 255 0" posX="0.0" posY="120.0"/>
        <content>Writeln("El área del cilindro es: " + (getValueOf(area))+" unidades cuadradas");	;	</content>
        <comment>"El área del cilindro es: " +area+ " unidades cuadradas" </comment>
        <connect ID="10"/>
        <io ioType="1" message="El área del cilindro es: " messageSuffix=" unidades cuadradas" newLine="true" variable="area"/>
      </block>
      <block id="10" type="IO">
        <options displayComment="true"/>
        <visual color="255 255 0" posX="0.0" posY="200.0"/>
        <content>Writeln("El volúmen del cilindro es: " + (getValueOf(volumen))+" unidades cúbicas");	;	</content>
